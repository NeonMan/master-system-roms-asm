; More "characters" made for special purposes.
; Encoded just like `font.inc`


; Progressbar characters
.macro font_progressbar
.db #0xFF, #0x00, #0x00, #0x00, #0x00, #0x00, #0xFF, #0x00 ; Progressbar Blank
.db #0xFF, #0x00, #0xFF, #0xFF, #0xFF, #0x00, #0xFF, #0x00 ; Progressbar Filled
.db #0x07, #0x04, #0x04, #0x04, #0x04, #0x04, #0x07, #0x00 ; Progressbar Start
.db #0xE0, #0x20, #0x20, #0x20, #0x20, #0x20, #0xE0, #0x00 ; Progressbar End
.endm
